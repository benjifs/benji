<p id="egg">Click an egg for a hint</p>
<noscript>
<p>Egg counting only works with JS enabled</p>
</noscript>
<script>
window.onload = function() {
	const EGGS = [{% for egg, hint in eggs %}'{{ egg }}'{% if not loop.last %},{% endif %}{% endfor %}]
	const eggs = window.eggs() || []
	for (const e of EGGS) {
		const $el = document.querySelector(`[data-egg="${e}"]`)
		if ($el && eggs.includes(e)) $el.className = 'found'
	}
	const found = document.querySelectorAll('.found')
	if (found.length == EGGS.length) {
		document.getElementById('egg').innerHTML = 'Congratulations! You have found all the eggs.'
	}
}
</script>
